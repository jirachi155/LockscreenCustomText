<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>items</key>
    <array>
        <dict>
            <key>cell</key>             <string>PSGroupCell</string>
            <key>footerText</key> <string>Type what you would like to see in the Lock Screen instead of "Press Home to unlock" (Leave empty for no text).</string>
            <key>footerAlignment</key> <integer>0</integer>
        </dict>
        <dict>
            <key>cell</key>             <string>PSSwitchCell</string>
            <key>default</key>          <true/>
            <key>defaults</key>         <string>com.giuzzidotcom.lockscreencustomtextpref</string>
            <key>key</key>              <string>LCTisEnabled</string>
            <key>label</key>            <string>Enabled</string>
            <key>PostNotification</key> <string>com.giuzzidotcom.lockscreencustomtextpref/settingschanged</string>
        </dict>
        <dict>
            <key>cell</key>             <string>PSEditTextCell</string>
            <key>defaults</key>         <string>com.giuzzidotcom.lockscreencustomtextpref</string>
            <key>key</key>              <string>LCTtext</string>
            <key>label</key>            <string>Lockscreen Text:</string>
            <key>placeholder</key>      <string>Press Home to unlock</string>
            <key>PostNotification</key> <string>com.giuzzidotcom.lockscreencustomtextpref/settingschanged</string>
        </dict>
    </array>
    <key>title</key>
    <string>LockScreen Custom Text iOS10</string>
</dict>
</plist>
